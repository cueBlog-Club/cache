{
    "name": "cuephp/cache",
    "type": "library",
    "description": "a light weight cache lib",
    "keywords": [ "psr-16", "cache", "light-cache","redis","memory"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CuePhp\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CuePhp\\Tests\\Cache": "tests/"
        }
    },
    "require": {
        "php": "~7.3 || ^8.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "predis/predis": "^1.1",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "provide": {
        "psr/simple-cache-implementation": "^1.0.0"
    }
}
